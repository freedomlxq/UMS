                                        <telerik:GridViewBoundColumnBase  Header="数量" Width="auto"  IsReadOnly="True" DataMemberBinding="{Binding ProCount}" IsFilterable="False"   />
                                      
                                    </telerik:RadGridView.Columns>
                                </telerik:RadGridView>

