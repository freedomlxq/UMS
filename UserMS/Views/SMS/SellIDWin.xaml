<telerik:RadWindow
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="UserMS.SellIDWin"
	xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
	Header="销售单号" WindowStartupLocation="CenterScreen"
	Width="164" Height="115.587" CanClose="False" HideMinimizeButton="True" HideMaximizeButton="True">

    <DockPanel x:Name="LayoutRoot">
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="销售单号" VerticalAlignment="Top" DockPanel.Dock="Top" />
        <TextBox x:Name="SellID" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" DockPanel.Dock="Top" />
        <telerik:RadButton x:Name="OK" Content="确定" HorizontalAlignment="Left" VerticalAlignment="Top" Click="OK_OnClick" />
        <telerik:RadButton x:Name="Cancel" Content="取消" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Cancel_OnClick" />
    </DockPanel>
</telerik:RadWindow>